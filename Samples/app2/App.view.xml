<mvc:View
	controllerName="com.mjzsoft.App"
	displayBlock="true"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
	xmlns="sap.m">
	<App id="app">
		<Page title="{AppViewModel>/title}">
			<VBox>
				<core:Icon color="#1873B4" src="sap-icon://sap-ui5" size="5rem" class="sapUiSmallMarginBottom" width="100%"/>
				<Title level="H1" titleStyle="H1" text="Hello UI5 World. Hurrah!" width="100%" textAlign="Center"/>
			</VBox>
		</Page>
	</App>
</mvc:View>